{
  "manifest_version": 3,
  "name": "Lark Full Width",
  "version": "1.0.0",
  "description": "Set Form Width 100%.",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
